{
    "name": "lilac-test-explorer",
    "displayName": "lilac-test-explorer",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.105.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "testing": {
            "testProviders": [
                {
                    "id": "lilac-tests",
                    "label": "Lilac Tests"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "pnpm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "pnpm run compile && pnpm run lint",
        "lint": "eslint src",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/vscode": "1.107.0",
        "@types/mocha": "10.0.10",
        "@types/node": "25.0.2",
        "@typescript-eslint/eslint-plugin": "8.50.0",
        "@typescript-eslint/parser": "8.50.0",
        "eslint": "9.39.2",
        "typescript": "5.9.3",
        "@vscode/test-cli": "0.0.12",
        "@vscode/test-electron": "2.5.2"
    }
}
